Module "TupleReplaceAtNAry Tests"
Versions
	"1.4.0"
Uses
	"Common"
Body

Test "TupleReplaceAtNAry ok" in primitives test suite is
[
	Require: <4, <5, 18, <1, 2, 3>, 11, 12>, 6>[2][3][1] → 0 =
    		<4, <5, 18, <0, 2, 3>, 11, 12>, 6>;
    	Require:
    		{4 → {4 → {4 → 4}}, 5 → {5 → {5 → 5}}, 6 → {6 → {6 → 6}}}[5][5][5] → 1 =
    			{4 → {4 → {4 → 4}}, 5 → {5 → {5 → 1}}, 6 → {6 → {6 → 6}}};
];

Test "TupleReplaceAtNAry fails with subscript-out-of-bounds exception" in primitives test suite if supported is
[
	Skip test
]
must raise subscript-out-of-bounds exception;

Test "TupleReplaceAtNAry fails with incorrect-argument-type exception" in primitives test suite if supported is
[
	Skip test
]
must raise incorrect-argument-type exception;

Test "TupleReplaceAtNAry fails with key-not-found exception" in primitives test suite if supported is
[
	Skip test
]
must raise key-not-found exception;

