/**
 * Make available {@code tuple}s and their basic type, {@code set}s and their
 * basic type, and the basic type of {@code map}.
 *
 * @author Todd L Smith &lt;anarakul@gmail.com&gt;
 */
Module
	"Collections"
Extends
Uses
	"Bootstrap",
	"Code",
	"Types"
Names
	"map type",
	"set",
	"set type",
	"tuple",
	"tuple type",
	"<>"
Body

/** The compiler needs this in order to specify "tuple type". */
privateTupleType ::= Special Object 35;

/**
 * @returns The basic type of any {@code tuple}.
 */
Method "tuple type" is
[
	privateTupleType;
] : meta;

/** The compiler needs this in order to specify "tuple". */
privateTuple ::= Special Object 34;

/**
 * @returns The type {@code tuple}.
 */
Method "tuple" is
[
	privateTuple;
] : tuple type;

/**
 * @return An empty tuple whose static type is merely {@code tuple}.
 */
Method "private <>" is
[
	Primitive 134;
] : tuple;

/**
 * @return An empty tuple.
 */
Method "<>" is
[
	Primitive 134;
] : type of private <>;

/** The compiler needs this in order to specify "set type". */
privateSetType ::= Special Object 29;

/**
 * @returns The basic type of any {@code set}.
 */
Method "set type" is
[
	privateSetType;
] : meta;

/** The compiler needs this in order to specify "set". */
privateSet ::= Special Object 28;

/**
 * @returns The type {@code set}.
 */
Method "set" is
[
	privateSet;
] : set type;

/** The compiler needs this in order to specify "map". */
privateMap ::= Special Object 21;

/**
 * @returns The basic type of any {@code map}.
 */
Method "map type" is
[
	privateMap;
] : meta;
