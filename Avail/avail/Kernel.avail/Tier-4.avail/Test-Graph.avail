System Module "Test-Graph"
Extends
Uses
	"Tier-3",
	"Graph"
Names
Body

StringGraph ::= Graph of string;

[
	gf : StringGraph Factory;
	gf := new StringGraph Factory;
	gf add node "hello";
	gf add node "there";
	gf add node "123";
	gf add edge from "hello" to "there";
/*	Print gf result;	*/
]();

gf ::= new StringGraph Factory;
gf add node "hello";
gf add node "there";
gf add edge from "hello" to "there";
g : StringGraph;
g := gf result;

Assert g nodes = {"there", "hello"};

