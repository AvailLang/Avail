/**
 * Return true if the supplied character is identified as an Arabic numeral
 * according to the Unicode character description, false otherwise.
 *
 * @method "_is an Arabic numeral"
 * @param "c" "character"
 * @returns "boolean"
 * @category "Unicode"
 */
Public stable method "_is an Arabic numeral" is
[
	c : character
|
	(c's code point) ∈ arabicNumerals
] : boolean;

/**
 * Return true if the supplied character is identified as a letter, false
 * otherwise.
 *
 * @method "_is a letter"
 * @param "character" "c"
 * @returns "boolean"
 * @category "Unicode"
 */
Public stable method "_is a letter" is 
[
 	c : character 
|
	cp ::= c's code point;

	if cp < 128 then 
	[
		65 ≤ cp ≤ 90 ∨ [97 ≤ cp ≤ 122]
	]
	else 
	[
		if cp < 8192 then 
		[
			192 ≤ cp ≤ 214
				∨ [216 ≤ cp ≤ 246
				∨ [248 ≤ cp ≤ 8191]]
		]
		else 
		[
			12352 ≤ cp ≤ 12799
				∨ [13312 ≤ cp ≤ 59391
				∨ [65136 ≤ cp ≤ 65277
				∨ [65313 ≤ cp ≤ 65338
				∨ [65345 ≤ cp ≤ 65370
				∨ [65377 ≤ cp ≤ 65503]]]]]
		]
	]

	/* TODO: [LAS] I believe this implementation (adapted from Mark's code) is
	 * incomplete, however I have not yet been able to find a complete listing
	 * of unicode "letters" by code point. 
	 */

] : boolean;

