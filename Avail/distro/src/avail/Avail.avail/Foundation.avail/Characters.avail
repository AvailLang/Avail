Private method "recase restriction for elements∈_using_" is
[
	range : natural number's type,
	converter : [character]→character
|
	[
		stringType : string's type
	|
		$body : type;
		If ⎡stringType⎤ = 0 then [Exit body with ""'s type];
		types : <character's type…|> := <>;
		For each index from 1 to |stringType's leading types| + 1 do
		[
			charType ::= stringType[index];
			types := eject types ++
				<if index ∈ range then
				[
					converted : {character|} := ∅;
					if |charType| is finite then
					[
						For each of charType's instances do
						[
							c : character
						|
							converted := eject converted + converter(c);
						];
						cast enumeration of converted
							into [t : character's type | t]
					]
					else [charType]
				]
				else [charType]>;
		];
		nonempty ::= cast types into [t : <character's type…|1..> | t];
		leading ::= nonempty[1..|nonempty|-1];
		default ::= nonempty[|nonempty|];
		<leading, default… | ||stringType||>
	] : type
];

/**
 * If a corresponding uppercase character is available, that character is
 * returned. Otherwise, the original character is returned.
 * 
 * @method "uppercase_"
 * @param "c" "character"
 * @returns "string"
 * @category "Unicode"
 */
Public stable method "uppercase_" is
[
	c : character
|
	(toUpperCase[c's code point] else [c's code point])→character
] : character;

/**
 * Answer an uppercased variant of the specified string.
 *
 * @method "uppercase_"
 * @param "s" "string"
 * @returns "string"
 * @category "Unicode"
 */
Stable method "uppercase_" is
[
	s : string
|
	map s through [c : character | uppercase c]
] : string;

Semantic restriction "uppercase_" is recase restriction
	for elements ∈ natural number
	using [c : character | uppercase c];

/**
 * Answer a capitalized variant of the specified string.
 *
 * @method "capitalize_"
 * @param "s" "string"
 * @returns "string"
 * @category "Unicode"
 */
Stable method "capitalize_" is
[
	s : string
|
	if |s| = 0 then [""] else [<uppercase (s[1])> ++ s[2..]]
] : string;

Semantic restriction "capitalize_" is recase restriction
	for elements ∈ 1's type
	using [c : character | uppercase c];

